<% content_for :head do %>
    <h1>Listing job_skills</h1>
<% end %>

<% content_for :main do %>
    <table>
      <tr>
        <th>Job ID</th>
        <th>Skill ID</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>

    <% @job_skills.each do |job_skill| %>
      <tr>
        <td><%= job_skill.job_id %></td>
        <td><%= job_skill.skill_id %></td>
        <td><%= link_to 'Show', job_skill %></td>
        <td><%= link_to 'Edit', edit_job_skill_path(job_skill) %></td>
        <td><%= link_to 'Destroy', job_skill, :confirm => 'Are you sure?', :method => :delete %></td>
      </tr>
    <% end %>
    </table>

    <br />

    <%= link_to 'New Job skill', new_job_skill_path %>
<% end %>